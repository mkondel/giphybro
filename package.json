{
  "homepage": "http://mkondel.github.io/giphybro",
  "name": "giphybro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o build",
    "start": "next start",
    "predeploy": "yarn build && touch build/.nojekyll && mv build/_next build/next && sed -i 's/_next/next/g' build/index.html build/404.html",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "next": "9.5.3",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "webpack": "^4.44.2"
  }
}
